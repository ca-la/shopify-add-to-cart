{
  "compilerOptions": {
    "downlevelIteration": true,
    "lib": ["es2015", "dom", "dom.iterable"],
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./docs/",
    "importHelpers": true,
    "noImplicitReturns": true,
    "strict": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react"
  },
  "include": [
    "./src/**/*"
  ]
}
